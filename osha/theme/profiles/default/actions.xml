<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
   
 <object name="site_actions" meta_type="CMF Action Category">
  <object name="worklist" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="plone">Worklist</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/@@worklist</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:member is not None</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>   
 </object>
 
 <object name="document_actions" meta_type="CMF Action Category">
  <object name="rss" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="rssfeeds" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">RSS</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$portal_url/@@rss-feeds</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>

 </object>   
 <object name="footer_actions" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="disclaimer" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Disclaimer</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$portal_url/disclaimer</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="copyright" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Copyright</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$portal_url/copyright</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
