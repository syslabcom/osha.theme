<div metal:define-macro="portlet">  
  <dl class="portlet" id="portlet_newsletter_static"
    tal:define="preflang here/portal_languages/getPreferredLanguage;
    link string:/$preflang/newsletter">
      


            <dt class="portletHeader" style="background-color: rgb(255, 210, 1);"><a href="#"
    tal:attributes="href link">Newsletter</a></dt>
            
            


            <dd class="portletItem" name="portletcontent" style="background-color: rgb(242, 227, 158);">
                <div class="portletContent">

          <span><a href="http://www.newoshera.eu/newsletter"
           tal:attributes="href link">NEW OSH ERA Newsletter is published twice a year</a>
          </span> 
             </div>



        </dd>
    </dl>
    
</div>

