<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
	lang="en"
	metal:use-macro="here/main_template/macros/master"
	i18n:domain="plone">

	<body>

		<div metal:fill-slot="main">

			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="title" value="new Title"/>         
				<input type="submit" class="context" name="form.button.UpdateTitle" value="set Title"/>
			</form>
			<form method="get"  tal:condition="python:context.portal_type=='Rich Document'"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="id" value="new id"/>         
				<input type="checkbox" name="guessLanguage" value="guessLanguage"/>Guess Language         
				<input type="submit" class="context" name="form.button.deleter" value="delete all"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="submit" class="context" name="form.button.setEnableNextPrevious" tal:condition="not:context/getNextPreviousEnabled" value="turn On previous/Next Navigation" />
				<input type="submit" class="context" name="form.button.setDisableNextPrevious" tal:condition="context/getNextPreviousEnabled" value="turn Off previous/Next Navigation" />
			</form>
			<form method="get" 
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="submit" class="context" name="form.button.setExcludeFromNav" tal:condition="not:context/getExcludeFromNav" value="Exclude from Nav" />
				<input type="submit" class="context" name="form.button.setIncludeInNav" tal:condition="context/getExcludeFromNav" value="Include in Nav" />
			</form>
			<form method="get" tal:condition="python:context.portal_type=='Rich Document'"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="submit" class="context" name="form.button.setRichDocAttachment" tal:condition="not:context/getDisplayAttachments" value="set RichDocAttachment" />
				<input type="submit" class="context" name="form.button.unsetRichDocAttachment" tal:condition="context/getDisplayAttachments" value="unset RichDocAttachment" />
			</form>
			<form method="get" tal:condition="python:context.portal_type=='Rich Document'"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="oldid" value="old id"/>         
				<input type="text" size="20" name="id" value="new id"/>         
				<input type="submit" class="context" name="form.button.ChangeId" value="change Id"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="submit" class="context" name="form.button.propagatePortlets" value="Propagate Portlets"/>
				<input type="submit" class="context" name="form.button.addLanguageTool" value="Add Languagetool"/>
				<input type="submit" class="context" name="form.button.reindexer" value="reindex object"/>
				<input type="submit" class="context" name="form.button.publisher" value="publish"/>
				<input type="submit" class="context" name="form.button.hider" value="hide"/>
			</form>

			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="label" value="label"/>         
				<input type="text" size="20" name="domain" value="domain"/>         
				<input type="submit" class="context" name="form.button.setTranslateTitle" value="Translate Title"/>
				<input type="submit" class="context" name="form.button.setTranslateDescription" value="Translate Description"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="id" value="id"/>         
				<input type="checkbox" name="excludeFromNav" value="excludeFromNav" checked="checked"/>exclude from Nav         
				<input type="submit" class="context" name="form.button.createFolder" value="create Folder"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="checkbox" name="recursive" value="recursive"/>recursive
				<input type="submit" class="context" name="form.button.fixTranslationReference" value="fix Translationreference"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="subtype" value="subtype"/>         
				<input type="submit" class="context" name="form.button.subtyper" value="subtype"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="id" value="Property id"/>         
				<input type="submit" class="context" name="form.button.delProperty" value="delete Property"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="manager" value="manager"/>         
				<input type="radio" name="cat" value="context"/> Context
				<input type="radio" name="cat" value="group"/> Group
				<input type="radio" name="cat" value="type"/> Typ
				<input type="text" size="20" name="id" value="status"/>         
				<input type="submit" class="context" name="form.button.blockPortlets" value="block Portlets"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="id" value="id"/>         
				<input type="text" size="20" name="typ" value="typ"/>         
				<input type="text" size="20" name="value" value="value"/>         
				<input type="submit" class="context" name="form.button.setProperty" value="set Property"/>
			</form>
			<form method="get" tal:condition="context/isPrincipiaFolderish"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<input type="text" size="20" name="sourcepath" value="sourcepath"/>         
				<input type="text" size="20" name="id" value="id"/>         
				<input type="text" size="20" name="targetpath" value="targetpath"/>         
				<input type="submit" class="context" name="form.button.cutAndPaste" value="cut and paste"/>
			</form>                         
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<textarea name="order" cols="20" rows="10"></textarea>
				<input type="submit" class="context" name="form.button.fixOrder" value="fix Order"/>
			</form>
			<form method="get"
				tal:attributes="action string:${context/absolute_url}/${view/__name__}">
				<textarea name="attrs" cols="20" rows="10"></textarea>
				<input type="submit" class="context" name="form.button.tranlateThis" value="translate this"/>
			</form>
			<div tal:condition="view/result">

				<!--h3>Results</h3-->
				<table id="report-table" tal:define="pl view/result" summary="Search & Replace results report">


					<tr>
						<!--th>&nbsp;</th-->
						<th>ID</th>
					</tr>
					<tal:block repeat="line pl">
					<tr>
						<!--td>
						<input type="checkbox" name="targets:list" tal:attributes="value string:${line}" />
						</td-->
						<td>
							<a href="" tal:attributes="href line" tal:content="line"></a>
						</td>
					</tr>
					</tal:block>
				</table>
			</div>



		</div>

	</body>
</html>

