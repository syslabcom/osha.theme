msgstr "Lestev pristavite k steni pod pravilnim kotom."

msgid "anim01_message_03"
msgstr "Ob vznožju visoke lestve naj vedno stoji ena oseba."

msgid "anim01_end"
msgstr "„Pazite – ne tvegajte!“"

msgid "anim02_end"
msgstr "„Pazite – ne tvegajte!“"

msgid "anim03_message_01"
msgstr "Izogibajte se delu v nevarnih ali neprimernih pogojih."

