#: This MessageCatalog is used especially for the generation of the web-charter of EW 2006.
#: In order to adapt the 'Sign the charter' section for a new EW,
#: just add a msgid named 'campaign_slogan_YYYY', where YYYY is the year.

#: Campaign Slogan EW2006
msgid "campaign_slogan_2006"
msgstr "Safe Start!"

#: EW Slogan
msgid "ew_slogan"
msgstr "Ġimgħa Ewropea għas-Saħħa u s-Sigurtà fuq il-Post tax-Xogħol${year}"

#: Title
msgid "certificate_title"
msgstr "Rikonoxximent"

#: Certificate for
msgid "certificate_for"
msgstr "Dan iċ-ċertifikat jirrikonoxxi li"

#: Contriubution
msgid "contribution_headline"
msgstr "inkiteb fil-Karta tal-Ġimgħa Ewropea b’impenn sabiex jikkontribwixxi b’mod attiv f’din is-sena ${year} "


#: Contribution
msgid "contribution_subline"
msgstr "<font face=\"ArialBold\">Kampanja tal-Ġimgħa Ewropea</font> <font face=\"ArialItalic\">\"${campaign_slogan}\"</font> by:"

#: RISQ portlet text
msgid "RISQ Zone for young people"
msgstr "RISQ Zone for young people"


