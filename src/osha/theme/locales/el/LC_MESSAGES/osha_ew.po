#: This MessageCatalog is used especially for the generation of the web-charter of EW 2006.
#: In order to adapt the 'Sign the charter' section for a new EW,
#: just add a msgid named 'campaign_slogan_YYYY', where YYYY is the year.

#: Campaign Slogan EW2006
msgid "campaign_slogan_2006"
msgstr "Safe Start!"

#: EW Slogan
msgid "ew_slogan"
msgstr "Eυρωπαϊκή Εβδομάδα για την Ασφάλεια και την Υγεία στην Εργασία ${year}"

#: Title
msgid "certificate_title"
msgstr "Βεβαίωση"

#: Certificate for
msgid "certificate_for"
msgstr "Με την παρούσα βεβαίωση πιστοποιείται ότι"

#: Contriubution
msgid "contribution_headline"
msgstr "έχει συνυπογράψει τη Χάρτα της Ευρωπαϊκής Εβδομάδας ${year} με τη δέσμευση να συμβάλει ενεργά στη φετινή"

#: Contribution
msgid "contribution_subline"
msgstr "<font face=\"ArialBold\">εκστρατεία Ευρωπαϊκής Εβδομάδας</font> <font face=\"ArialItalic\">\"${campaign_slogan}\"</font> by:"

#: RISQ portlet text
msgid "RISQ Zone for young people"
msgstr "RISQ Zone for young people"

