<tal:manager replace="structure provider:collage.ContentManager" />

<dl tal:define="url here/absolute_url;
    obj_description here/Description;
    title_or_id here/pretty_title_or_id;
    effective here/getEffectiveDate;
    plone context/@@plone;
    oshaview here/@@oshaview;
    collage_id view/getCollageId;
    url python:oshaview.handleOSHMailUrls(url, collage_id);
    toLocalizedTime nocall:plone/toLocalizedTime">


    <dt>
        <img src="publication_icon.gif" 
            tal:attributes="src string:${plone/navigationRootUrl}/publication_icon.gif"/>
        <a href="#"
            tal:attributes="href string:$url/view" tal:content="title_or_id">
        </a>
   </dt>
  
    <dd>
    <span tal:content="structure python:toLocalizedTime(effective)"> 
        &nbsp; 
    </span>

    <!--br/>
    <span class="description" tal:content="structure obj_description">
        description 
    </span-->
    
    </dd>

</dl>