body {
    -x-system-font: none;
    font-size-adjust: none;
    font-stretch: normal;
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    line-height: normal;
    margin: 0.75em 0 0.25em;
    font: 13px Verdana, Helvetica, "Lucida Grande", Lucida, Arial, sans-serif;
    color: #191919;
    margin: 0;
    padding: 0;
    background: url("<dtml-var portal_url>/top.gif") repeat-x scroll left -20px rgba(0, 0, 0, 0);
}
/* Outlook background http://www.campaignmonitor.com/blog/post/1777/body-background-images-outlook/ */

v\:* {
    behavior: url(#default#VML);
    display: inline-block;
}

#visual-portal-wrapper {
    overflow-x: hidden;
    overflow-y: auto;
    position: relative;
    overflow-x: hidden;
    margin: 0 auto;
    width: 960px;
    padding: 0;
}

a {
    background-color: transparent;
    color: #144989;
    text-decoration: none;
}

img {
    border: medium none;
    vertical-align: middle;
}

.visualClear {
    clear: both;
    display: block;
}

p {
    margin: 0 0 0.75em;
}

h1, h2, h3, h4, h5, h6 {
    color: #191919;
    font: 13px/20px Verdana,Helvetica,"Lucida Grande",Lucida,Arial,sans-serif;
    margin: 0 0 12px;
}

h1 {
  font: 18px/35px Verdana,Helvetica,"Lucida Grande",Lucida,Arial,sans-serif;
  text-shadow: 2px 2px 5px #DDD;
}

h2 { font-weight: bold; };

.socialMediaOSHMAILfg {
    background: #F2F4F6 none repeat scroll 0 0;
    padding: 10px 10px 10px 10px;
}

.socialMediaOSHMAILfg a {
    font: 12px Verdana,Helvetica,"Lucida Grande",Lucida,Arial,sans-serif;
}

.collage-item {
    font: 12px Verdana,Helvetica,"Lucida Grande",Lucida,Arial,sans-serif;
}

.boxing_styles_inner_content {
    font-size: 100%;
    background-color: #F7F8F9;
    border: 1px solid #C5CBD8;
    font-size: 100%;
}

/* oshmail button styles */

.button {
    display: inline-block;
    /* zoom and *display = ie7 hack for display:inline-block */
    zoom: 1;
    *display: inline;
    vertical-align: baseline;
    margin: 0 2px;
    outline: none;
    cursor: pointer;
    text-align: center;
    text-decoration: none;
    font: 14px/100% Arial, Helvetica, sans-serif;
    padding: .5em 2em .55em;
    text-shadow: 0 1px 1px rgba(0,0,0,.3);
    -webkit-border-radius: .5em;
    -moz-border-radius: .5em;
    border-radius: .5em;
    -webkit-box-shadow: 0 1px 2px rgba(0,0,0,.2);
    -moz-box-shadow: 0 1px 2px rgba(0,0,0,.2);
    box-shadow: 0 1px 2px rgba(0,0,0,.2);
}

.button.darkblue {
    color: #d9eef7 !important;
    border: solid 1px #003399;
    background: #999ada;
    background: -webkit-gradient(linear, left top, left bottom, from(#999ada), to(#003399));
    background: -moz-linear-gradient(top,  #999ada,  #003399);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#999ada', endColorstr='#003399');
}

.button.darkblue:hover {
    background: #52126d;
    background: -webkit-gradient(linear, left top, left bottom, from(#001e8e), to(#061454));
    background: -moz-linear-gradient(top,  #001e8e,  #061454);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#001e8e', endColorstr='#061454');
}

.button.darkblue:active {
    color: #80bed6;
    background: -webkit-gradient(linear, left top, left bottom, from(#003399), to(#999ada));
    background: -moz-linear-gradient(top,  #003399, #999ada);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#003399', endColorstr='#999ada');
}

.button.gray {
  background: #eee;
}

.oshmail-blog-icon {
    color: #d9eef7 !important;
    font-size: 20px;
    padding: 2px 5px;
    font-weight: bold;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    position: relative;
    top: -8px;
}
/* end oshmail button styles */


