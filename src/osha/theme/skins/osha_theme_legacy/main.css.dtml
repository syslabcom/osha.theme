}

div#faq_results a.faq_toggle {
    font-weight: bold;
    font-size: 13px;
}

div#faq_results li.faq_result {
    list-style-type: none;
    list-style-image: url("treeCollapsed.gif");
}

div#faq_results li.expanded {
    list-style-type: none;
    list-style-image: url("treeExpanded.gif");
}

/* #3209 on't activate the carousel if there is only one item
   lipstick.css: .jcarousel-clip width: 548px; padding-left: 31px
   and #competitionSchwusch li: padding-left: 180px; padding-right 90px
   the carousel is activated as soon as there is anything hidden
   548px + 31px - 180px - 90px = 309px
 */
body.template-competitions-view #competitionSchwusch li {
    width: 309px;
}

/* Simple news view for Belgian sites */
div.teaserdate {
    font-family: Courier;
    padding-bottom: 1em;
}

/* overlay, copied from sunburst's public.css */
div.overlay {
    display: none;
    font-size: 80%;
    height: auto;
    margin: 1em;
    width: auto;
}
div.overlay-iframe, div.overlay-ajax {
    min-height: 100px;
    width: 60%;
    z-index: 99;
}
div.overlay-iframe {
    height: 60%;
}
div.overlaybg div.close, div.overlay div.close {
    background-image: url("pb_close.png");
    cursor: pointer;
    height: 30px;
    left: -14px;
    position: absolute;
    top: -14px;
    width: 30px;
}
.pb-ajax {
    overflow-y: auto;
}
.pb-ajax, .pb-image, .overlay iframe {
    background-color: #FFFFFF;
    border: 1px solid #999999;
    box-shadow: 0 0 3em 0.5em #666666;
    white-space: normal;
}
.pb-ajax > div {
    padding: 1em;
    width: 92%;
}
.pb-ajax .documentActions {
    display: none;
}
div.overlay div.close span {
    display: block;
    height: 1px;
    margin: -1px 0 0 -1px;
    overflow: hidden;
    padding: 0;
    width: 1px;
}


/* dynatree */
div.flatlist-template {
    display: none;
}
div.ui_controls .sparse{
    display: none;
}

div.flatlist_container{
  padding: 4px 2px;
}
.dynatree-atwidget .treepopup{
background: url("search_icon.png") no-repeat scroll 2px 1px White;
    border: 1px solid #AAB1C5;
    color: #191919;
    cursor: pointer;
    font-size: 95%;
    margin-bottom: 1px !important;
    padding: 1px 1px 1px 19px;
    text-transform: none;
    width: 50px;
}
ul.dynatree-container,
div.flatlist_container.opened{
   background-color: #E5E6E8;
   border: none;
   font-size: 100%;
}
div.dynatree-atwidget span.delete{
  background: url("ico_check_red.gif") no-repeat scroll 0 0 transparent;
  padding: 0 8px;
}
div.dynatree-atwidget span.delete span {
  display: none
}
div.dynatree-atwidget span.toggleButton{
    font-weight: bold;
}
div.dynatree-atwidget span.toggleButton.opened{
    background: url("minus_icon.gif") no-repeat scroll 0 4px #E5E6E8;
}
a.fakeLink{
    color: #000000;
}
a.fakeLink:hover{
    color: #A8126A;
    cursor: pointer;
}
div.dynatree-custom.disabled{
  display: none;
}
span.dynatree-selected a{
  color: #A8126A;
}
div.flatlist_container .flatlist-item {
   border-top: 1px solid #8095a8;
   background: #02020c;
   background: -webkit-gradient(linear, left top, left bottom, from(#627281), to(#02020c));
   background: -webkit-linear-gradient(top, #627281, #02020c);
   background: -moz-linear-gradient(top, #627281, #02020c);
   background: -ms-linear-gradient(top, #627281, #02020c);
   background: -o-linear-gradient(top, #627281, #02020c);
   padding: 0 8px;
   -webkit-border-radius: 8px;
   -moz-border-radius: 8px;
   border-radius: 8px;
   -webkit-box-shadow: rgba(0,0,0,1) 0 1px 0;
   -moz-box-shadow: rgba(0,0,0,1) 0 1px 0;
   box-shadow: rgba(0,0,0,1) 0 1px 0;
   text-shadow: rgba(0,0,0,.4) 0 1px 0;
   color: white;
   white-space: nowrap;
   margin: 2px;
   text-decoration: none;
   vertical-align: middle;
   float:left;
   }

