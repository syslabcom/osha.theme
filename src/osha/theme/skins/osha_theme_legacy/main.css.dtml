}
.portletItemDetails {
    text-align:left;
}

#abovecontent-portlets dl.portlet {
    background: transparent none repeat scroll 0% 0%;
}
#belowcontent-portlets dl.portlet {
    background: transparent none repeat scroll 0% 0%;
}
