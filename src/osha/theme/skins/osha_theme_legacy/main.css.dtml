    margin-left: 0;
}
#siteactions-wrapper h2 {
    color: &dtml-linkColor;;
    font-size:1em;
    margin:0;
    padding: 0 1em;
    border-bottom: none;
}
#osha-topdropdowns {
    margin:10px 104px 0 201px;
    position:relative;
}
#portal-network {
    padding:0 1em;
}
#portal-siteactions {
    font-size: 10px;
}

.dropdown {
    background: transparent url('&dtml-portal_url;/select.gif') 0% 50% no-repeat;
    padding-left: 1.5em;
    float: left;
    margin-right: 1em;
}
.dropdown select {
    font-size: 94%;
}

select[multiple] {
    background: white;
    /* Fixes a problem on Chrome where the background of the multiselect
     * widget in a portlet was grey, making it difficult to see selected items.
     */
}

