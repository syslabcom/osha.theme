.visualClearRight {
    clear: right;
}
#portal-header {
    border-bottom:1px solid &dtml-contentViewBorderColor;;
    background:&dtml-contentViewBackgroundColor; url('&dtml-portal_url;/topbanner2.jpg') 100% 50% no-repeat;
    float:left;
    height:92px;
    width:100%;
    margin-top:-1px; /* compensate 1px height hidden DIV with Skip-to-... links */
    position:relative;
}
#portal-logo {
    float:left;
}
#select_language_form {
    display: block;
    float: right;
}
